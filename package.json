{
  "name": "discord-slash",
  "version": "1.0.0",
  "description": "Slash Command helpers",
  "author": "Joe Dailey <josephldailey@gmail.com>",
  "license": "MIT",
  "main": "bin/index.js",
  "files": [
    "bin/**/*"
  ],
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "nodemon --watch index.ts --exec ts-node index.ts",
    "build": "rm -r bin; tsc --build tsconfig.json",
    "rebuild": "tsc --build tsconfig.json",
    "test": "jest",
    "clean": "rm bin -r"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@types/node": "^16.11.9",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsc": "^2.0.3"
  }
}
